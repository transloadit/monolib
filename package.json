{
  "name": "monolib",
  "private": true,
  "scripts": {
    "pub": "npm-run-all test lernaPublish",
    "lernaPublish": "lerna publish",
    "new": "sucrase-node new.ts",
    "lernaBootstrap": "lerna bootstrap --npm-client=yarn",
    "install": "npm run lernaBootstrap",
    "fix": "DEBUG=eslint:cli-engine eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint": "DEBUG=eslint:cli-engine eslint . --ext .js,.jsx,.ts,.tsx",
    "build": "lerna run tsc",
    "test:watch": "jest packages/*/src/*.test.ts --watch",
    "test": "npm-run-all build lernaBootstrap lint jest",
    "jest": "jest --verbose packages/*/src/*.test.ts"
  },
  "dependencies": {
    "execa": "5.0.0",
    "inflection": "1.12.0",
    "inquirer": "8.0.0",
    "replace": "1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/eslint-parser": "^7.13.10",
    "@babel/eslint-plugin": "^7.13.10",
    "@sucrase/jest-plugin": "^2.2.1",
    "@types/jest": "^28.1.3",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "eslint": "^7.22.0",
    "eslint-config-transloadit": "^2.0.0",
    "eslint-import-resolver-typescript": "^3.1.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^24.3.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-import": "^0.0.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fakefile": "1.0.0",
    "jest": "^28.1.1",
    "lerna": "4.0.0",
    "npm-run-all": "4.1.5",
    "replace-require-with-import": "^2.1.0",
    "sucrase": "^3.25.0",
    "ts-migrate": "^0.1.30",
    "typescript": "^4.7.4"
  }
}
